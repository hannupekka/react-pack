{
  "name": "react-pack",
  "version": "1.0.0",
  "description": "React project skeleton",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build": "check-node-version --package && npm run clean && npm run build:webpack",
    "dev": "NODE_ENV=development node server/devServer.js",
    "start": "node server/server.js",
    "lint": "eslint src && jscs src",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "postinstall": "npm run build"
  },
  "author": "Hannu-Pekka Heinäjärvi <hannupekka@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression": "^1.6.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.1.2",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "helmet": "^2.1.2",
    "history": "^2.1.2",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "normalize.css": "^4.2.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.10.0",
    "react": "^15.3.0",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "recompose": "^0.20.2",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
