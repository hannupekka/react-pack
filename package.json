{
  "name": "react-pack",
  "version": "1.0.0",
  "description": "React project skeleton",
  "scripts": {
    "ban": "ban",
    "build:test": "npm run lint && npm run test",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build": "check-node-version --package && npm run clean && npm run build:webpack",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development node server/devServer.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint src/**/*.js*",
    "prepush": "npm run ban && npm run lint & npm run test",
    "start": "node server/server.js",
    "test:coverage": "TEST_RUNNER=true jest --coverage",
    "test:update": "TEST_RUNNER=true jest -u",
    "test:watch": "TEST_RUNNER=true npm test -- --watch",
    "test": "TEST_RUNNER=true jest"
  },
  "author": "Hannu-Pekka Heinäjärvi <hannupekka@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "check-node-version": "^3.2.0",
    "compression": "^1.7.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "dom4": "^2.0.1",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.1.0",
    "imports-loader": "^0.8.0",
    "isomorphic-fetch": "^2.2.1",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.5",
    "mini-css-extract-plugin": "^0.4.0",
    "normalize.css": "^8.0.0",
    "normalizr": "^3.2.3",
    "postcss-fixes": "^2.0.1",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-css-modules": "^4.7.2",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^4.0.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-console-groupify": "^0.0.8",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-console-log-variable-names": "^0.0.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "ban-sensitive-files": "^1.9.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eventsource-polyfill": "^0.9.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "nock": "^9.2.3",
    "react-test-renderer": "^16.3.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^3.1.1"
  },
  "engines": {
    "yarn": "^1.1.0",
    "node": "^9.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "tools"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js",
      "^.+\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./tools/jest/setup.js",
    "verbose": true
  }
}
