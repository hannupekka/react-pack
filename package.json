{
  "name": "react-pack",
  "version": "1.0.0",
  "description": "React project skeleton",
  "scripts": {
    "ban": "ban",
    "build:test": "npm run lint && npm run test",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build": "check-node-version --package && npm run clean && npm run build:webpack",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development node server/devServer.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint src/**/*.js*",
    "prepush": "npm run ban && npm run lint & npm run test",
    "start": "node server/server.js",
    "test:coverage": "TEST_RUNNER=true jest --coverage",
    "test:update": "TEST_RUNNER=true jest -u",
    "test:watch": "TEST_RUNNER=true npm test -- --watch",
    "test": "TEST_RUNNER=true jest"
  },
  "author": "Hannu-Pekka Heinäjärvi <hannupekka@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "check-node-version": "^2.1.0",
    "compression": "^1.7.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "dom4": "^1.8.5",
    "exports-loader": "^0.6.4",
    "express": "^4.16.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.6.3",
    "helmet": "^3.8.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.3",
    "postcss-fixes": "^2.0.1",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "recompose": "^0.25.1",
    "redux": "^3.7.2",
    "redux-logic": "^0.12.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-console-groupify": "^0.0.4",
    "babel-plugin-transform-console-log-variable-names": "^0.0.3",
    "ban-sensitive-files": "^1.9.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "eventsource-polyfill": "^0.9.6",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "nock": "^9.0.22",
    "react-test-renderer": "^16.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "webpack-dev-server": "^2.9.1"
  },
  "engines": {
    "yarn": "^1.1.0",
    "node": "^9.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "tools"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js",
      "^.+\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./tools/jest/setup.js",
    "verbose": true
  }
}
