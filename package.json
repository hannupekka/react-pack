{
  "name": "react-pack",
  "version": "1.0.0",
  "description": "React project skeleton",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build": "check-node-version --package && npm run clean && npm run build:webpack",
    "dev": "NODE_ENV=development webpack-dashboard node server/devServer.js",
    "start": "node server/server.js",
    "flow": "flow",
    "lint": "./tools/flow_annotation_check.sh && eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "postinstall": "npm run build"
  },
  "author": "Hannu-Pekka Heinäjärvi <hannupekka@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-flow-react-proptypes": "^0.12.2",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "check-node-version": "^1.1.1",
    "compression": "^1.6.2",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.6.3",
    "helmet": "^2.1.2",
    "history": "^2.1.2",
    "html-webpack-plugin": "^2.22.0",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "normalize.css": "^4.2.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "recompose": "^0.20.2",
    "redux": "^3.4.0",
    "redux-api-middleware": "^1.0.2",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "babel-jest": "^15.0.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.1",
    "eventsource-polyfill": "^0.9.6",
    "jest": "^15.1.1",
    "nock": "^8.0.0",
    "pre-commit": "^1.1.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.1",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.12.1"
  },
  "pre-commit": [
    "lint",
    "flow",
    "test"
  ],
  "engines": {
    "npm": "3.10.3",
    "node": "6.5.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "tools"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupTestFrameworkScriptFile": "tools/jest/setup.js",
    "verbose": true
  }
}
