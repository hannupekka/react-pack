{
  "name": "react-pack",
  "version": "1.0.0",
  "description": "React project skeleton",
  "scripts": {
    "clean": "rimraf dist",
    "ban": "ban",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build": "check-node-version --package && npm run clean && npm run build:webpack",
    "dev": "NODE_ENV=development webpack-dashboard node server/devServer.js",
    "start": "node server/server.js",
    "flow": "flow check",
    "flow:trace": "flow check --traces 5",
    "lint": "./tools/flow_annotation_check.sh && eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:watch": "npm test -- --watch",
    "heroku-postbuild": "npm run build"
  },
  "author": "Hannu-Pekka Heinäjärvi <hannupekka@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "check-node-version": "^2.0.1",
    "compression": "^1.6.2",
    "cross-env": "^3.2.4",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "dom4": "^1.8.3",
    "exports-loader": "^0.6.4",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.6.3",
    "helmet": "^3.5.0",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.7.6",
    "imports-loader": "^0.7.1",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.2",
    "lodash": "^4.17.4",
    "normalize.css": "^6.0.0",
    "normalizr": "^3.2.2",
    "postcss-fixes": "^1.4.2",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "recompose": "^0.22.0",
    "redux": "^3.4.0",
    "redux-observable": "^0.14.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.1-smooth",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "ban-sensitive-files": "^1.9.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "flow-bin": "^0.42.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "nock": "^9.0.11",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.5.4",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-logger": "^3.0.0",
    "redux-mock-store": "^1.2.2",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.4.2"
  },
  "pre-commit": [
    "ban",
    "lint",
    "flow",
    "test"
  ],
  "engines": {
    "npm": "3.10.3",
    "node": "6.5.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "tools"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js",
      "^.+\\.(css|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./tools/jest/setup.js",
    "verbose": true
  }
}
